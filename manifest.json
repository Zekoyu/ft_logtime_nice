{
	"manifest_version": 2,
	"name": "ft_logtime",
	"version": "1.5",
	"description": "Calculates your logtime for the current month on intra.42.fr.",
	"icons": {
		"48": "icons/42-logo.png"
	},
	"content_scripts": [
		{
			"matches": [
				"https://profile.intra.42.fr/*"
			],
			"js": [
				"ft_logtime.js"
			]
		}
	],
	"browser_specific_settings": {
		"gecko": {
			"id": "ft_logtime-1.4@intra.42.fr"
		}
	}
}
