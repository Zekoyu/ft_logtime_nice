{
	"manifest_version": 2,
	"name": "Logtime",
	"version": "1.1",
	"description": "Calculates your logtime for the current month on intra.42.fr.",
	"content_scripts": [
		{
			"matches": [
				"https://profile.intra.42.fr/*"
			],
			"js": [
				"logtime.js"
			]
		}
	],
	"browser_specific_settings": {
		"gecko": {
			"id": "logtime@intra.42.fr"
		}
	}
}
